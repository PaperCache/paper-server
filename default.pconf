# Defaults to localhost
host=127.0.0.1

# Default port
port=3145

# Maximum size of the cache specified in bytes
max_size=2GiB

# Policies which should be considered by the cache
# Pipe-separated list
# Possible values:
# - lfu
# - lru
# - mru
# - fifo
policies=lfu|lru|mru|fifo

# Maximum number of concurrent connections
max_connections=4
